<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <TextView 
        android:id="@+id/order_chooser_noorder"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
		android:gravity="center"
	    android:text="@string/spicture_myorders_noorder"
        />
    <ListView 
        android:id="@+id/order_chooser_list"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:divider="@null"/>
	<com.jing.app.jjgallery.viewsystem.publicview.WaveSideBarView
		android:id="@+id/order_chooser_side"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:visibility="gone"/>
</RelativeLayout>
